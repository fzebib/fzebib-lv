server {

    listen 80;
    root /usr/share/nginx/www/kibanalive/src;
    server_name localhost;
    access_log /var/log/nginx/localhost_access.log;
    error_log /var/log/nginx/localhost_error.log;


    location / {

        proxy_set_header Host $host; proxy_set_header X-Real-IP $remote_addr; proxy_pass http://127.0.0.1:5601; index index.html index.htm index.js;
    }
}
